{
	"name": "Slate",
	"type": "dark",
	"semanticHighlighting": true,
	"colors": {
		"editor.background": "#0e0e15",
		"editor.foreground": "#a7a4af"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#829297",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string.quoted.single",
				"string.quoted.double",
				"string.template"
			],
			"settings": {
				"foreground": "#b1d36d"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#ffcb6b"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"keyword.control"
			],
			"settings": {
				"foreground": "#767a92"
			}
		},
		{
			"name": "Import keywords",
			"scope": [
				"keyword.control.import",
				"keyword.control.export",
				"keyword.control.from"
			],
			"settings": {
				"foreground": "#e0a2d3"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#E60063"
			}
		},
		{
			"name": "Arithmetic operators",
			"scope": [
				"keyword.operator.arithmetic"
			],
			"settings": {
				"foreground": "#E60063"
			}
		},
		{
			"name": "Assignment operators",
			"scope": [
				"keyword.operator.assignment"
			],
			"settings": {
				"foreground": "#E60063"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.other"
			],
			"settings": {
				"foreground": "#a7a4af"
			}
		},
		{
			"name": "Parameters",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#a7a4af"
			}
		},
		{
			"name": "Properties",
			"scope": [
				"variable.other.property",
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#e0a2d3"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#73bf9c"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.class",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#e0a2d3"
			}
		},
		{
			"name": "Interfaces",
			"scope": [
				"entity.name.type.interface"
			],
			"settings": {
				"foreground": "#cba6f7"
			}
		},
		{
			"name": "Types",
			"scope": [
				"entity.name.type",
				"support.type"
			],
			"settings": {
				"foreground": "#cba6f7"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"entity.name.function.decorator",
				"punctuation.decorator"
			],
			"settings": {
				"foreground": "#ffcb6b"
			}
		},
		{
			"name": "Regex",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#FF6188",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation"
			],
			"settings": {
				"foreground": "#767a92"
			}
		},
		{
			"name": "variable.language.this",
			"scope": [
				"variable.language.this"
			],
			"settings": {
				"foreground": "#ffcb6b"
			}
		}
	],
	"semanticTokenColors": {
		"comment": "#829297",
		"string": "#b1d36d",
		"number": "#ffcb6b",
		"regexp": "#FF6188",
		"keyword": "#767a92",
		"operator": "#E60063",
		"variable": "#a7a4af",
		"parameter": "#a7a4af",
		"property": "#e0a2d3",
		"function": "#73bf9c",
		"method": "#73bf9c",
		"class": "#e0a2d3",
		"interface": "#cba6f7",
		"enum": "#cba6f7",
		"enumMember": "#91aac0",
		"type": "#cba6f7",
		"typeParameter": "#cba6f7",
		"namespace": "#a7a4af",
		"decorator": "#ffcb6b",
		"macro": "#ffcb6b",
		"*.deprecated": {
			"fontStyle": "strikethrough"
		},
		"*.readonly": {
			"fontStyle": "italic"
		}
	}
}
	"tokenColors": [
		{
			"name": "Source",
			"scope": ["source", "text"],
			"settings": {
				"foreground": "#a7a4af"
			}
		},
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#383d51",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": [
				"string.quoted.single",
				"string.quoted.double",
				"string.template",
				"string.quoted.other"
			],
			"settings": {
				"foreground": "#aac17b"
			}
		},
		{
			"name": "String Escape",
			"scope": ["constant.character.escape", "constant.other.placeholder"],
			"settings": {
				"foreground": "#FFD866"
			}
		},
		{
			"name": "String Interpolation",
			"scope": [
				"punctuation.definition.template-expression",
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#78DCE8"
			}
		},
		{
			"name": "Regular Expression",
			"scope": ["string.regexp"],
			"settings": {
				"foreground": "#FF6188",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric",
				"constant.numeric.integer",
				"constant.numeric.float",
				"constant.numeric.hex",
				"constant.numeric.octal",
				"constant.numeric.binary"
			],
			"settings": {
				"foreground": "#33b3cc"
			}
		},
		{
			"name": "Boolean & Null",
			"scope": [
				"constant.language",
				"constant.language.boolean",
				"constant.language.null",
				"constant.language.undefined"
			],
			"settings": {
				"foreground": "#33b3cc"
			}
		},
		{
			"name": "Keyword - if",
			"scope": ["keyword.control.conditional", "keyword.control.loop"],
			"settings": {
				// "foreground": "#f47e98",
				// "foreground": "#fccffc",
				"foreground": "#e3e1e8a8",
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword - export",
			"scope": ["keyword.control.export"],
			"settings": {
				"foreground": "#91aac0",
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword - 'return', 'function'",
			"scope": ["keyword.control.flow", "storage.type.function"],
			"settings": {
				"foreground": "#7d7a8b",
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword - import",
			"scope": [
				// "keyword.control.import",
				// "storage.type.type.ts",
				"keyword.operator.expression.import.ts"
			],
			"settings": {
				// "foreground": "#f47e98",
				"foreground": "#d09bd2",
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.logical",
				"keyword.operator.wordlike"
			],
			"settings": {
				// "foreground": "#f47e98",
				"foreground": "#afd1e9cf",
				"fontStyle": ""
			}
		},

		{
			"name": "Operator",
			"scope": ["keyword.operator", "punctuation.accessor"],
			"settings": {
				"foreground": "#e60063"
			}
		},
		{
			"name": "Operator Special",
			"scope": [
				"keyword.operator.assignment",
				"keyword.operator.arithmetic",
				"keyword.operator.comparison",
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#e60063"
			}
		},
		{
			"name": "Storage Type",
			"scope": ["storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#5b6467",
				"fontStyle": ""
			}
		},
		{
			"name": "Storage Modifier",
			"scope": [
				"storage.modifier.package",
				"storage.modifier.import",
